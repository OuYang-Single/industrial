<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:binding="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="com.cn.setuplibrary.viewmodel.EngineeringParametersViewModel" />

    </data>
<androidx.core.widget.NestedScrollView
    android:layout_width="match_parent"
    android:paddingTop="17dp"
    android:paddingBottom="17dp"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/content"
        android:paddingRight="24dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PID参数设置"
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:id="@+id/tv_pid_title"
            android:textSize="11sp"
            />
        <LinearLayout
            android:id="@+id/ll_pid"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_pid_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_width="0dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                app:shape="rectangle"
                app:shape_radius="2dp"
                android:layout_marginRight="6dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_pid_p"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="P"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />
                <EditText
                    android:id="@+id/et_pic_p"
                    android:hint="请输入"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:layout_marginHorizontal="9dp"
                    android:gravity="center|right"
                    android:text="@={viewModel.engineerIngBean.pinP}"
                    android:textColorHint="#ff909090"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    android:background="@null"
                    android:layout_toRightOf="@+id/tv_pid_p"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:background="#00F30000"
                    binding:onClickCommand="@{viewModel.onClickPinP}"
                    android:id="@+id/ll_pid_p"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" />


            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_width="0dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                app:shape="rectangle"
                app:shape_radius="2dp"
                android:layout_marginRight="6dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_pid_i"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="I"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />
                <EditText
                    android:id="@+id/et_pic_i"
                    android:hint="请输入"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:layout_marginHorizontal="9dp"
                    android:gravity="center|right"
                    android:text="@={viewModel.engineerIngBean.pinI}"
                    android:textColorHint="#ff909090"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    android:background="@null"
                    android:layout_toRightOf="@+id/tv_pid_i"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <LinearLayout
                    android:background="#00FFFFFF"
                    binding:onClickCommand="@{viewModel.onClickPinP}"
                    android:id="@+id/ll_pid_i"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" />

            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_width="0dp"
                android:layout_height="34dp"
                android:layout_weight="1"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_pid_c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="D"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />
                <EditText

                    android:id="@+id/et_pic_d"
                    android:hint="请输入"
                    android:layout_marginHorizontal="9dp"
                    android:gravity="center|right"
                    android:text="@={viewModel.engineerIngBean.pinD}"
                    android:textColorHint="#ff909090"
                    android:textColor="#ff909090"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:textSize="11sp"
                    android:background="@null"
                    android:layout_toRightOf="@+id/tv_pid_c"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

                <LinearLayout
                    android:background="#00FFFFFF"
                    binding:onClickCommand="@{viewModel.onClickPinP}"
                    android:id="@+id/ll_pid_d"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>
        <TextView
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_pid"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="运行参数设置"
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:id="@+id/tv_operating_parameters_title"
            android:textSize="11sp"
            />

        <LinearLayout
            android:id="@+id/ll_operating_parameters"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_operating_parameters_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                android:id="@+id/rv_start_mode"
                binding:onClickCommand="@{viewModel.onClickStartMode}"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_temperature_mode_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="温度模式"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_temperature_mode_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_mode_name"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_temperature_mode_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.temperatureMode}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                binding:onClickCommand="@{viewModel.onClickFillingTime}"
                android:id="@+id/rv_filling_time"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_filling_time_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="填水时间"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_filling_time_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_filling_time"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_filling_time_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.fillingTime}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>

        <LinearLayout

            android:id="@+id/ll_delay_pump_start_time"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_operating_parameters"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                binding:onClickCommand="@{viewModel.onClickPumpJammingTime}"
                android:layout_marginRight="9dp"
                android:id="@+id/rv_delay_pump_start_time"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_delay_pump_start_time_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="延时开泵时间"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_delay_pump_start_time_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_delay_pump_start_time"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_delay_pump_start_time_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.pumpJammingTime}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                binding:onClickCommand="@{viewModel.onClickCoolingTemperature}"
                android:id="@+id/rv_set_cooling_temperature"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_set_cooling_temperature_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设定冷却温度"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_set_cooling_temperature_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_set_cooling_temperature"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_set_cooling_temperature_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.coolingTemperature}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_set_exhaust_pressure"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_delay_pump_start_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                binding:onClickCommand="@{viewModel.onClickExhaustPressure}"
                android:layout_marginRight="9dp"
                android:id="@+id/rv_set_exhaust_pressure"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_set_exhaust_pressure_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设定排气压力"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_set_exhaust_pressure_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_set_exhaust_pressure"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_set_exhaust_pressure_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.exhaustPressure}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:id="@+id/rv_set_emptying_time"
                binding:onClickCommand="@{viewModel.onClickEmptyingTime}"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_set_emptying_time_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设定排空时间"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_set_emptying_time_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_set_emptying_time"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_set_emptying_time_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.emptyingTime}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>

        <TextView
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_set_exhaust_pressure"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="故障参数设置"
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:id="@+id/tv_fault_parameters_title"
            android:textSize="11sp"
            />
        <LinearLayout
            android:id="@+id/ll_temperature_too_high"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_fault_parameters_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                binding:onClickCommand="@{viewModel.onClickHighDeviation}"
                android:id="@+id/rv_temperature_too_high"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_temperature_too_high_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="温度过高偏差"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_temperature_too_high_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_temperature_too_high"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_temperature_too_high_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.highDeviation}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                binding:onClickCommand="@{viewModel.onClickLowDeviation}"
                android:id="@+id/rv_low_temperature_deviation"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_low_temperature_deviation_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="温度低偏差"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_low_temperature_deviation_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_low_temperature_deviation"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_low_temperature_deviation_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.lowDeviation}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_temperature_difference_value"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_temperature_too_high"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                binding:onClickCommand="@{viewModel.onClickTemperatureDifferencVlue}"
                android:id="@+id/rv_temperature_difference_value"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_temperature_difference_value_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="出回煤温差"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_temperature_difference_value_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_temperature_difference_value_high"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_temperature_difference_value_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.engineerIngBean.temperatureDifference}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                binding:onClickCommand="@{viewModel.onClickTemperatureDeviationTime}"
                android:id="@+id/rv_temperature_deviation_time"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_temperature_deviation_time_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="温度偏差时间"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_temperature_deviation_time_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="15dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:id="@+id/tv_temperature_deviation_time"
                    android:layout_marginRight="6dp"
