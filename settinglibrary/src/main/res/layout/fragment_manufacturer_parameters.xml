<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:binding="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="com.cn.setuplibrary.viewmodel.ManufacturerParametersViewModel" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/content"
        android:paddingLeft="9dp"
        android:paddingRight="18dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_marginTop="18dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="常规"
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:id="@+id/tv_operating_parameters_title"
            android:textSize="11sp"
            />

        <LinearLayout
            android:id="@+id/ll_operating_parameters"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_operating_parameters_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                android:id="@+id/rv_start_mode"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_temperature_mode_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="机种设置"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:visibility="invisible"
                    android:id="@+id/img_temperature_mode_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_mode_name"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_temperature_mode_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.manufactorBean.typeMachine}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:id="@+id/rv_filling_time"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_filling_time_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设定温度上限"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_filling_time_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_filling_time"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_filling_time_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.manufactorBean.temperatureUpperLimit}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_delay_pump_start_time"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_operating_parameters"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                android:id="@+id/rv_delay_pump_start_time"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_delay_pump_start_time_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设置温度下限"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_delay_pump_start_time_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_delay_pump_start_time"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_delay_pump_start_time_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.manufactorBean.temperatureOffline}"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:visibility="invisible"
                android:id="@+id/rv_set_cooling_temperature"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_set_cooling_temperature_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="填水时间"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_set_cooling_temperature_arrow"
                    android:layout_marginRight="10dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="20dp"
                    android:layout_height="20dp"/>
                <TextView
                    android:id="@+id/tv_set_cooling_temperature"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_set_cooling_temperature_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="请选择"
                    android:textColor="#ff909090"
                    android:textSize="11sp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>

        <TextView
            android:layout_marginTop="14dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_delay_pump_start_time"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本"
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:id="@+id/tv_version_title"
            android:textSize="11sp"
            />

        <LinearLayout
            android:id="@+id/ll_software_version_information"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_version_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                android:id="@+id/rv_software_version_information_mode"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_software_version_information_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="软件版本信息"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:visibility="invisible"
                    android:id="@+id/img_software_version_arrow"
                    android:layout_marginRight="1dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_software_version_name"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_software_version_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="XX.20240426V0002"
                    android:textColor="#ff909090"
                    android:textSize="10dp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:id="@+id/rv_activation_date_time"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_activation_date_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="激活日期"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:id="@+id/img_activation_date_arrow"
                    android:layout_marginRight="1dp"
                    android:visibility="invisible"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_activation_date_time"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_activation_date_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.manufactorBean.activationDate}"
                    android:textColor="#ff909090"
                    android:textSize="10dp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_trial_permission"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/ll_software_version_information"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:layout_marginRight="9dp"
                android:id="@+id/rv_trial_permission"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_trial_permission_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="试用权限"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:visibility="invisible"
                    android:id="@+id/img_trial_permission_arrow"
                    android:layout_marginRight="1dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_trial_permission"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_trial_permission_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="#ff909090"
                    android:textSize="10dp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
            <com.hjq.shape.layout.ShapeRelativeLayout
                android:id="@+id/rv_activate_serial_number"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="34dp"
                app:shape="rectangle"
                app:shape_radius="2dp"
                app:shape_solidColor="#CC000C36"
                app:shape_strokeColor="#1B56A5"
                app:shape_strokeWidth="0.33dp" >
                <TextView
                    android:layout_marginLeft="12dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/tv_activate_serial_number_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="激活序列号"
                    android:textColor="#ffffffff"
                    android:textSize="11sp"
                    />

                <ImageView
                    android:visibility="invisible"
                    android:id="@+id/img_activate_serial_number_arrow"
                    android:layout_marginRight="1dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ic_right_arrows"
                    android:layout_width="1dp"
                    android:layout_height="1dp"/>
                <TextView
                    android:id="@+id/tv_activate_serial_number"
                    android:layout_marginRight="6dp"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/img_activate_serial_number_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.manufactorBean.activationCode}"
                    android:textColor="#ff909090"
                    android:textSize="10dp"
                    />
            </com.hjq.shape.layout.ShapeRelativeLayout>
        </LinearLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>